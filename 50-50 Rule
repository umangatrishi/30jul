#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--){
        int X,Y;
        cin>>X>>Y;
        if(X<50)
        std::cout << "Z" << std::endl;
        else if(Y<50 && X>=50)
        std::cout << "F" << std::endl;
        else
        std::cout << "A" << std::endl;
    }

}
